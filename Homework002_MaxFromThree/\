//Программа принимает на вход 3 числа и выдает максимальное из них

Console.WriteLine("Введите число a: ");
int a = Convert.IntTo32(Console.ReadLine());

Console.WriteLine("Введите число b: ");
int b = Convert.IntTo32(Console.ReadLine());

Console.WriteLine("Введите число c: ");
int c = Convert.IntTo32(Console.ReadLine());

max = a;

if (b>max) max=b;
if (c>max) max=c;

Console.WriteLine("a=" + a + ";b=" + b + ";c=" + c + " ->" + max);
